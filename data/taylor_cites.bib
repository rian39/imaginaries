@article{ ISI:000342978500005,
Author = {Rajao, Raoni and Duque, Ricardo B.},
Title = {{Between Purity and Hybridity: Technoscientific and Ethnic Myths of
   Brazil}},
Journal = {{SCIENCE TECHNOLOGY \& HUMAN VALUES}},
Year = {{2014}},
Volume = {{39}},
Number = {{6, SI}},
Pages = {{844-874}},
Month = {{NOV}},
Abstract = {{This article examines the foundation myths of Brazil in the last two
   centuries, paying particular attention to the relationship between these
   myths and governmental attitudes toward the hybridity of Northern and
   Southern ethnic and technoscientific entities. Based upon this
   examination, the article argues that it is important to consider both
   the wider temporal frames and the shifts and sedimentations that have
   formed current foundation myths and shaped their relation to science and
   technology. Postcolonial science technology studies theories illuminate
   aspects of this trajectory, but our analysis suggests a more complex
   scenario that involves internal political dynamics and the work of local
   intellectuals. We argue that the example of Brazilian social scientists
   should encourage scholars to go beyond the current focus on breaking the
   myths of technoscience and undertake mythmaking initiatives with wider
   societal resonances.}},
