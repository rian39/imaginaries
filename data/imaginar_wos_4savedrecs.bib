@article{ ISI:000081337000012,
Author = {Levy, A},
Title = {{Groups and group analysis: the question of belief}},
Journal = {{REVUE FRANCAISE DE PSYCHANALYSE}},
Year = {{1999}},
Volume = {{63}},
Number = {{3}},
Pages = {{891+}},
Month = {{JUL-SEP}},
Abstract = {{in so far as it is a place where processes for establishing social
   links can be observed and possibly reworked, the restricted group is
   also identified with values, and all the more so because it can be seen
   as an entity in itself, abstracted from the social reality of which it
   is an element.
   A rereading of Group psychology and analysis of the ego stresses the
   connection between the different figures of the group (from the
   primitive group to the organised group, between the imaginary and
   action inscribed in society, governed by rational thought), warning us
   against the risk of confusing them as if it was always a question of
   one and same object. It also enables us to underline the narrow
   relations existing between the development of groups and that of the
   individual psychology that only exists with reference to the group
   psychology from which it stems.
   The question of belief and its status is nevertheless relevant to all
   these questions that notably concern the practices of group analysis,
   in the context of training, psychotherapy or intervention.}},
ISSN = {{0035-2942}},
Unique-ID = {{ISI:000081337000012}},
}

@article{ ISI:000081482900012,
Author = {Fischer, MH and Deubel, H and Wohlschlager, A and Schneider, WX},
Title = {{Visuomotor mental rotation of saccade direction}},
Journal = {{EXPERIMENTAL BRAIN RESEARCH}},
Year = {{1999}},
Volume = {{127}},
Number = {{2}},
Pages = {{224-232}},
Month = {{JUL}},
Abstract = {{This investigation studied the latencies of saccadic eye movements that
   were directed away from a target by a variable angular distance, which
   was given by instruction. Such a movement presumably requires an
   intentional, visuomotor mental rotation of the saccade vector,
   resulting in prolonged reaction times. From a study on the control of
   directed hand movements, it has been hypothesized that all visuomotor
   and visual mental rotation tasks share a common processing stage. We
   tested this hypothesis with a saccade task in which subjects shifted
   their gaze either towards (0 degrees, pro-saccade), or 30, 60, 90, 120,
   150, or 180 degrees (anti-saccade) away from a randomly cued position
   on an imaginary clock face. With four different cueing conditions,
   latencies increased monotonically with required gaze shift from 0-150
   degrees, thus exhibiting a mental rotation latency pattern. However, we
   also found anti-saccades faster than 150 degrees gaze shift and slower
   rotation speeds with peripheral cues than with central cues. Together
   with the overall shallower latency increase compared with previous
   findings with mental rotation tasks, these results cast doubt on the
   notion of a common, central processing mechanism for the different
   types of tasks.}},
ISSN = {{0014-4819}},
Unique-ID = {{ISI:000081482900012}},
}

@article{ ISI:000081476500002,
Author = {Bonne, O and Canetti, L and Bachar, E and De-Nour, AK and Shalev, A},
Title = {{Childhood imaginary companionship and mental health in adolescence}},
Journal = {{CHILD PSYCHIATRY \& HUMAN DEVELOPMENT}},
Year = {{1999}},
Volume = {{29}},
Number = {{4}},
Pages = {{277-286}},
Month = {{SUM}},
Abstract = {{An imaginary companion (IC) is a frequently encountered childhood
   fantasy, invisible to anyone but the child, who may be named, addressed
   or played with. Whether the presence of an IC is a normal developmental
   feature has not been determined. We examined psychometric measures and
   the presence/absence of childhood TC in a sample of 850 mentally
   healthy adolescents. 17.6\% of our subjects, more often females,
   reported having had such a companion. Subjects who reported having had
   an IC in childhood exhibited higher levels of distress and emotional
   discontrol, displayed prolonged transitional object attachment and
   immature modes of coping with stress. Thus, although childhood
   imaginary companionship is not indicative of psychopathology, it may
   denote a vulnerability for adolescent perturbation and difficulty in
   coping with emotionally laden situations.}},
ISSN = {{0009-398X}},
Unique-ID = {{ISI:000081476500002}},
}

