@article{ ISI:000074514700005,
Author = {Risselada, R},
Title = {{The discourse functions of sane: Latin marker of agreement in
   description, interaction and concession}},
Journal = {{JOURNAL OF PRAGMATICS}},
Year = {{1998}},
Volume = {{30}},
Number = {{2}},
Pages = {{225-244}},
Month = {{AUG}},
Abstract = {{Apart from its use as a manner adverb with full lexical meaning ('in a
   healthy, sensible way'), Latin sane is used as a discourse particle
   that is traditionally associated in grammars of Latin with a number of
   seemingly unconnected functions. In this paper I propose a more unified
   account, in which its various discourse functions are described on the
   basis of a three-stage analysis, which distinguishes between a
   relatively abstract basic meaning ('positive evaluation'), the domains
   or discourse levels to which this particle may pertain, and discourse
   pragmatic properties of the utterances in which sane is used. Special
   attention is devoted to the alleged use of sane as a concessive marker.
   I argue that, in the instances involved, sane is not in itself a
   concessive marker, but is used in the first part of a paratactic
   concessive constellation to mark the speaker's (restricted) agreement
   with a speech act that is attributed to a real or imaginary
   interlocutor.
   The paper starts with a more theoretical discussion of the approach to
   discourse particles on which my account of sane is based. The
   concluding section contains a number of diachronic observations on the
   development of the various discourse functions from each other and from
   the use of sane as a manner adverb.}},
ISSN = {{0378-2166}},
Unique-ID = {{ISI:000074514700005}},
}

@article{ ISI:000074222700006,
Author = {Pickering, N},
Title = {{Imaginary restrictions}},
Journal = {{JOURNAL OF MEDICAL ETHICS}},
Year = {{1998}},
Volume = {{24}},
Number = {{3}},
Pages = {{171-175}},
Month = {{JUN}},
Abstract = {{The role of literature and imagination in medicine and medical ethics
   is currently under discussion. This paper argues that the role of
   literature is not to furnish generalisable examples for guidance.
   Rather, engagement with literature parallels moral engagement with
   other people. The work of the imagination, in this context, is not to
   hypothesise, but to grant life to the characters and world of
   literature. In doing this, one may develop one's moral life.}},
ISSN = {{0306-6800}},
Unique-ID = {{ISI:000074222700006}},
}

