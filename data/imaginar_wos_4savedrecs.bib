@article{ ISI:A1992JU80400017,
Author = {LYON, DR},
Title = {{POSITION-LINKED INTERFERENCE IN FORMING SIMPLE VISUAL GROUPS}},
Journal = {{JOURNAL OF EXPERIMENTAL PSYCHOLOGY-HUMAN PERCEPTION AND PERFORMANCE}},
Year = {{1992}},
Volume = {{18}},
Number = {{4}},
Pages = {{1139-1157}},
Month = {{NOV}},
Abstract = {{Experiments with 2-element visual groups reveal a form of interference
   that has several interesting properties. Observers judged the
   orientation of an imaginary line formed by 2 pixels (the target) while
   attempting to ignore a third pixel (the noise). Noise interfered with
   performance, even when it was made distinct from the target in various
   ways. This interference was strongly position linked; a single equation
   described the interference pattern for many different target-noise
   configurations. Maximum interference was observed not when the noise
   was closest to the target but when it was at a distance of half the
   separation between target pixels. Some initial findings were consistent
   with the idea that visual grouping reflects the operation of visual
   channels with Gabor-like receptive-field profiles. But subsequent
   results implicated processes that automatically construct visual
   boundaries.}},
ISSN = {{0096-1523}},
Unique-ID = {{ISI:A1992JU80400017}},
}

@article{ ISI:A1992JU50300003,
Author = {HARTER, S and CHAO, C},
Title = {{THE ROLE OF COMPETENCE IN CHILDRENS CREATION OF IMAGINARY FRIENDS}},
Journal = {{MERRILL-PALMER QUARTERLY-JOURNAL OF DEVELOPMENTAL PSYCHOLOGY}},
Year = {{1992}},
Volume = {{38}},
Number = {{3}},
Pages = {{350-363}},
Month = {{JUL}},
Abstract = {{The function of young children's imaginary friends was investigated
   from the perspective of perceived competence. Two possible processes
   were examined in which children create imaginary companions who are
   either (a) more competent than themselves and then function as
   ego-ideals with whom children can identify; or (b) less competent than
   themselves so that by comparison the children appear more competent,
   and then can teach or aid less competent companions. As predicted,
   teachers judged children with imaginary friends to be less competent
   and less socially accepted by peers. Children's descriptions of
   themselves and their imaginary friends on the Pictorial Scale for
   Perceived Competence and Social Acceptance for Young Children revealed
   striking gender differences: Boys created supercompetent companions who
   were more competent than themselves, whereas girls created incompetent
   companions who were less competent than themselves. The implications
   for sex role stereotypes were explored.}},
ISSN = {{0272-930X}},
Unique-ID = {{ISI:A1992JU50300003}},
}

@article{ ISI:A1992JR37600006,
Author = {CAILLAVET, C and MINCHOM, M},
Title = {{THE IMAGINARY MESTIZO - CLASSIFICATORY IDEALS AND SOCIORACIAL
   STRATEGIES IN LATIN-AMERICA (16TH-20TH C)}},
Journal = {{HOMME}},
Year = {{1992}},
Volume = {{32}},
Number = {{2-4}},
Pages = {{115-132}},
Month = {{APR-DEC}},
Abstract = {{The socio-racial categories (Indian, Mestizo, White) used in the
   documentary sources and scientific literature of Latin America from the
   16th century to the present day, are highly ambiguous. This article
   sets out to analyse the variations (semantic and substantive) in these
   protean categories. The theoretical framework which served as a model
   for Colonial society in Latin America was a transposition of rigid
   concepts from metropolitan Spain where the obsession with << purity of
   blood >> and the essential criterion of the necessary separation of the
   races precluded the integration of the concept of race mixture. The
   contradictions inherent in this denial of the Colonial reality can be
   observed in the official classification of the censuses, the parish
   registers and the Inquisition records. A less familiar type of source,
   the << Declaration of Mestizo >> sheds light on the experience of the
   individuals concerned, and reveals the adaptive strategies developed to
   << play the system >> and overcome the constraints of caste society.
   (Trad. M.M.)}},
ISSN = {{0439-4216}},
Unique-ID = {{ISI:A1992JR37600006}},
}

@article{ ISI:A1992JR37600009,
Author = {MOLINIEFIORAVANTI, A},
Title = {{TRANSATLANTIC COMPARISONS}},
Journal = {{HOMME}},
Year = {{1992}},
Volume = {{32}},
Number = {{2-4}},
Pages = {{165-183}},
Month = {{APR-DEC}},
Abstract = {{Anthropologists have overlooked Spanish elements in Latin American
   cultures. Andean examples are used to present the problem of mestizo
   culture; and the contributions and inadequacies of various theoretical
   approaches are assessed.     Through case studies and comparisons by
   conceptual fields, light is shed on the mestizo processes at work in
   the belief in the << fat-taker >>, which has haunted the Andean
   imagination for centuries. Owing to its symbolic effectiveness in
   Andean culture, the theme of fat lends itself to the propagation of
   images having to do with the pragmatic value of human fat in
   17th-century European medicine. Pre-Columbian sacrificial practices and
   the colonial zoomorphization of Indians give a meaning to this. This
   mestizo belief has been worked out through a complicated combination of
   practices and images, which blurrs the boundary between what is real
   and what is imaginary.}},
ISSN = {{0439-4216}},
Unique-ID = {{ISI:A1992JR37600009}},
}

