@article{ ISI:A1992JH02200004,
Author = {CHASTAIN, G},
Title = {{ANALOG VERSUS DISCRETE SHIFTS OF ATTENTION ACROSS THE VISUAL-FIELD}},
Journal = {{PSYCHOLOGICAL RESEARCH-PSYCHOLOGISCHE FORSCHUNG}},
Year = {{1992}},
Volume = {{54}},
Number = {{3}},
Pages = {{175-181}},
Month = {{AUG}},
Abstract = {{Two discrimination experiments were run to investigate analog versus
   discrete properties of a shift of visual spatial attention. Central
   cuing was used in Experiment 1, whereas peripheral cuing was used in
   Experiment 2. Presentation of a probe stimulus between fixation and the
   target (Distance 1), opposite fixation from the target (Distance 3), or
   away from an imaginary line running from the target through fixation
   (Distance 2) permitted a fine-grained analysis of attention at those
   loci across target-probe delays. D-prime analyses in both experiments
   suggest that attention is shifted in a discrete manner between
   locations. Sensitivity to probes was generally greater when the probe
   was aligned with the target and fixation, with Distance 3 equal to
   Distance 1, than when it was away (at Distance 2). Analysis of
   sensitivity to targets across cue-probe delays suggests that attention
   was directed to the probe upon its appearance.}},
ISSN = {{0340-0727}},
Unique-ID = {{ISI:A1992JH02200004}},
}

@article{ ISI:A1992JF18200016,
Author = {GREEN, A},
Title = {{PRELIMINARY TO A DISCUSSION ON THE FUNCTION OF THEORY IN PSYCHOANALYTIC
   TRAINING}},
Journal = {{REVUE FRANCAISE DE PSYCHANALYSE}},
Year = {{1992}},
Volume = {{56}},
Number = {{2}},
Pages = {{507-514}},
Month = {{APR-JUN}},
Abstract = {{In contemporary psychoanalytic training, theory is always mentioned
   last of all and its role always minimised in relation to the personal
   analysis and to supervision. It is treated with mistrust and seen as a
   source of resistance, especially in the case when one is dealing with
   authors belonging to other psychoanalytic sub-groups. Each analyst is
   invested with an imaginary power (by way of the transference) and with
   a real power (by way of his institutional capacities). A third -
   symbolic - power is granted to the theoretician analyst, whose
   influence is not to be underestimated. The corpus of Freud's work has
   been fragmented since his death  into a certain number of subsystems
   which compete with each other and in which it is difficult to
   distinguish true innovations from neo-formations whose role is to
   legitimate alternatives to the unbearable nature of analytic truth.
   Only time will do justice to the attempts to block challenging new
   ideas and to prove the fallacious character of certain conceptions
   which have won favour by their novelty.}},
ISSN = {{0035-2942}},
Unique-ID = {{ISI:A1992JF18200016}},
}

