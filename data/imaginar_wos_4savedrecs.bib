@article{ ISI:A1992JX23600003,
Author = {GIRAUDO, MD and PERUCH, P},
Title = {{ANALYSIS OF THE COGNITIVE-PROCESSES INVOLVED IN THE SPATIAL
   REPRESENTATION OF UNSKILLED ADULTS}},
Journal = {{TRAVAIL HUMAIN}},
Year = {{1992}},
Volume = {{55}},
Number = {{2}},
Pages = {{135-153}},
Month = {{JUN}},
Abstract = {{The aim of this study was to study the cognitive processes involved in
   spatial representation of unskilled adults (i.e., assembly-line workers
   at a sugar refinery) in relation to a reference population. During the
   first stage, the subjects were asked to locate twelve sites, all of
   which were within the subject's navigational space (i.e., the company
   grounds for unskilled subjects and the downtown area of the city of
   Marseille for reference subjects). The experiment was repeated two days
   after. In the second stage, both unskilled and reference subjects were
   asked to locate twelve sites learned by studying a map of an imaginary
   city. The accuracy of their responses was measured using a
   two-dimensional regression method.
   Results showed that the cognitive processes involved in spatial
   representation are more elementary than is generally admitted. The
   intra-individual variability of responses revealed that the study of
   these processes can only be carried out through several repetitions
   with the same subject. Moreover, unskilled adults differed from the
   reference population more for the space learned by reading the map than
   for the space learned by displacement. Lastly, for both groups the
   involved cognitive processes were different according to the mode of
   spatial representation acquisition.}},
ISSN = {{0041-1868}},
Unique-ID = {{ISI:A1992JX23600003}},
}

@article{ ISI:A1992JW51800017,
Author = {VICTOR, JS},
Title = {{RITUAL ABUSE AND THE MORAL CRUSADE AGAINST SATANISM}},
Journal = {{JOURNAL OF PSYCHOLOGY AND THEOLOGY}},
Year = {{1992}},
Volume = {{20}},
Number = {{3}},
Pages = {{248-253}},
Month = {{FAL}},
Abstract = {{This article addresses the research question: What accounts for
   widespread belief in allegations of -ritual- child abuse by satanic
   cults in the absence of any verifiable law enforcement or scientific
   evidence? The hypothesis is that allegations of ritual abuse are
   manifestations of the social construction of an imaginary form of
   deviance which is being promoted by a moral crusade against satanism.
   Events of a satanic cult ritual abuse scare in England are used to
   illustrate the collective behavior dynamics. The interpretation
   suggests that the activism of moral crusaders is a response to belief
   in frames for attributing meaning to an ambiguous problem, linked to
   shared concerns for fighting ``evil{''} in society. The conclusion is
   that controversies surrounding claims about ritual child abuse can be
   best understood if they are investigated in the social context of the
   moral crusade against satanism.}},
ISSN = {{0091-6471}},
Unique-ID = {{ISI:A1992JW51800017}},
}

@article{ ISI:A1992JR61700003,
Author = {MISTLBERGER, K},
Title = {{CONTOURS OF A NEW EUROPEAN SECURITY-ORDER}},
Journal = {{OSTERREICHISCHE ZEITSCHRIFT FUR POLITIKWISSENSCHAFT}},
Year = {{1992}},
Volume = {{21}},
Number = {{3}},
Pages = {{261-272}},
Abstract = {{After the revolutionary changes in Europe, the dissolution of the old
   structures and the emergence of new risks, the building of a new
   European security-order became a priority political task. The new order
   develops out of an evolutionary process of change which rests on the
   maintenance, and particularly on cautious reforms, institutionalization
   and revitalization of existing organisations and concepts in the field
   of European security. The CSCE, at the begin seen as the roof of an
   European security-architecture, suffers from structural problems, but
   can find rewarding tasks in the areas of crisis-management,
   conflict-prevention and disarmament. The European Community has begun,
   in close co-operation with the revalorized WEU, to engage more
   intensively in problems of European security and aims at the
   development of an independent European defense-identity. However, at
   the moment Europe can hardly do without the United States as a leading
   power and the (reformed) NATO. The much conjured new European
   security-architecture is, as the case of Jugoslavia shows, not yet
   working. Up to now it is an imaginary thing, of which only contours are
   recognizable.}},
ISSN = {{0378-5149}},
Unique-ID = {{ISI:A1992JR61700003}},
}

