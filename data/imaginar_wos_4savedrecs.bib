@article{ ISI:000079271900003,
Author = {Birouste, J and Leroy-Viemon, B and Moragues, JL},
Title = {{The restoration of the beyond-subject to reconstruct self-esteem}},
Journal = {{EVOLUTION PSYCHIATRIQUE}},
Year = {{1998}},
Volume = {{63}},
Number = {{4}},
Pages = {{601-614}},
Month = {{OCT-DEC}},
Abstract = {{The article develops the hypothesis according to which self-esteem is
   based on the ``pictogrammic{''} activity specific to the originary
   process, it is then possible to establish among patients having
   identity difficulties, a therapeutic approach which takes the dynamic
   dimension of the drive into account.
   The authors relate the establishment of a pattern concerning the
   restoration of the beyond-subject to the Freudian theory. They remind
   us of the operational value of the two following heterogeneous psychic
   strategies i.e. the activities of representation and escape such as
   they appear in Freud's dialectics through the process of pain reduction
   provoked in the infant by the tension of need. Then they go deeper into
   the idea that motility participates in a domain potentially able to
   register and keep traces of an irrepresentable relationship to the
   other. For them motility appears as the mode of expression of urgent
   need spreading libidinal capacity over the surface of an ``awakened{''}
   body.
   These studies open out onto the creation of the pattern of a space
   heterogeneous to the psyche yet sufficiently homogeneous to it to
   represent the interface with the unknown real. They tend to be
   validated by the results produced by a clinic of the ``link{''} which
   identifies and deals with the strength or the failures of self-esteem
   implied in ego depression.
   Starting from two clinical examples, the clinicians show how an
   activated originary process, which is neither of the imaginary nor
   symbolic domain, restores links beyond the subject which are yet the
   basis for the evidence of being on which the subject then builds his
   relationships and self-esteem. The study of Denise's case in the field
   of aging buttresses the argument of motility as a means of expression
   able to connect peacefully the strangeness of the real and the
   familiarity of the psychic apparatus, and consequently, to propose a
   foundation for the restoration of self-esteem. The study of Gregory's
   case, in the domain of high-level sports, allows to refer motility to
   the dynamic dimension of drive.}},
ISSN = {{0014-3855}},
Unique-ID = {{ISI:000079271900003}},
}

@article{ ISI:000079461900008,
Author = {Jain, RK},
Title = {{Indian diaspora, globalisation and multiculturalism: A cultural analysis}},
Journal = {{CONTRIBUTIONS TO INDIAN SOCIOLOGY}},
Year = {{1998}},
Volume = {{32}},
Number = {{2}},
Pages = {{337-360}},
Month = {{JUL-DEC}},
Abstract = {{The paper focuses on Indian diaspora as a case study of globalisation
   and multiculturalism. It raises the question of a structural and
   historical distinction between the socio-cultural pluralism of
   societies like India and the ones overseas where Indian populations
   migrated and serried, and answers it in terms of dialectically related
   civilisations and settlement societies. A cultural analysis, using the
   comparative and theoretical approach of socio-cultural anthropology,
   lends to the positing of a `field of forces' paradigm to orientate and
   position empirical instances of Indian diaspora globally. Three major
   issues affecting the overseas Indian communities are explored, namely,
   difference and translation, hybridity and creolisation, and policies of
   multiculturalism. The conclusion underscores factors such as the
   locationality of the analyst, rite general pacifist orientation of
   diaspora communities and slippage between the imaginary and the
   imagined in relation to India that characterise Indian diaspora and
   influence irs sociological study.}},
ISSN = {{0069-9667}},
Unique-ID = {{ISI:000079461900008}},
}

@article{ ISI:000079376000009,
Author = {Kaney, S and Bowen-Jones, K and Bentall, RP},
Title = {{Persecutory delusions and autobiographical memory}},
Journal = {{BRITISH JOURNAL OF CLINICAL PSYCHOLOGY}},
Year = {{1999}},
Volume = {{38}},
Number = {{Part 1}},
Pages = {{97-102}},
Month = {{MAR}},
Abstract = {{Objectives. It was hypothesized that people with persecutory delusions,
   such as depressed people, would experience difficulty when attempting
   to generate specific autobiographical memories.
   Design. 20 deluded participants, 20 depressed patients and 20 normal
   controls were compared on an autobiographical memory test.
   Methods. Participants attempted to recall memories to positive and
   negative cue words. Independent raters classified responses as
   specific, general, imaginary or unscorable. Over-general memories were
   further classified as extended or categorical.
   Results. Deluded participants recalled significantly more general
   (especially categorical) memories and fewer specific memories than the
   normal control subjects. Depressed participants showed little evidence
   of over-general memories, although they showed faster recall latencies
   to negative than to positive cues.
   Conclusions. People with paranoid delusions show information processing
   features similar to those previously reported in people with
   depression. The possible contribution of adverse experiences to
   over-general autobiographical recall is discussed.}},
ISSN = {{0144-6657}},
Unique-ID = {{ISI:000079376000009}},
}

