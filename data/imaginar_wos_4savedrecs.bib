@article{ ISI:A1993LL72300002,
Author = {SCHOUSBOE, I},
Title = {{EVIL PLAY - A BROADER VIEW OF PLAY AND ITS FUNCTIONS}},
Journal = {{NORDISK PSYKOLOGI}},
Year = {{1993}},
Volume = {{45}},
Number = {{2}},
Pages = {{97-119}},
Month = {{JUN}},
Abstract = {{Scholars of different theoretical persuasions seem united in
   considering imaginary play an `'all good'' activity with unambiguously
   positive functions for the current well-being as well as for the
   longterm development of the child. This article questions the
   assumption that play can have only positive functions.
   The paper first tries to establish the existence of games which may
   reasonably be characterized as evil. Next it identifies certain
   conceptual problems which seem to facilitate the tendency to overlook
   possible negative functions of play. To help overcome these problems,
   the paper proposes a general model of the spheres of reality within
   which the playing child operates. On the basis of this model, play is
   analysed as an activity that may enchant the child to move beyond his
   or her normal limits in a way which may or may not be culturally
   accepted. It is concluded that play can have not only cathartic or
   neutral functions with regard to expressing and elaborating on evil.
   Under certain circumstances it can also generate evil tendencies.
   Finally it is pointed out that certain characteristics of contemporary
   Western society probably promote the willingness among adults,.
   including scholars, to conceive play as an all-good activity and to
   expect the child to be an exceptionally uncomplicated and good-natured
   member of society.}},
ISSN = {{0029-1463}},
Unique-ID = {{ISI:A1993LL72300002}},
}

@article{ ISI:A1993LM28800006,
Author = {NAKAMURA, M and MASUDA, M and INAGAKI, M},
Title = {{INFLUENCES OF KNOTS AND GROOVES ON PSYCHOLOGICAL IMAGES OF WOOD
   WALL-PANELS}},
Journal = {{MOKUZAI GAKKAISHI}},
Year = {{1993}},
Volume = {{39}},
Number = {{2}},
Pages = {{152-160}},
Abstract = {{Influences of groove intervals, knot-diameters, and knot-arrangements
   on psychological images were studied. Panel-type patterns and room-type
   patterns with various knots were prepared by means of computer
   image-generation techniques. Questionnaires regarding psychological
   images of the patterns were distributed. Results were as follows: 1)
   The most `'agreeable'' interval of grooves is near 1/6 to 1/10 of the
   geometrical average of the vertical and horizontal sizes of panels.
   This result corresponds to our previous report'' using panels without
   knots. 2) The influences of sizes and numbers of knots on an
   `'agreeable'' interval is small. 3) The most `'agreeable'' interval of
   grooves in the room-type patterns is the same as that in the panel-type
   patterns, and that interval is about 20 cm in imaginary size. 4) The
   smaller the knot ratio, the more `'agreeable'' the wall-panel becomes.
   (The knot-ratio is an area fraction between knots and a panel.) 5) The
   proper amount of randomness in the arrangement of knots gives
   relatively `'agreeable'' and `'beautiful'' images, and the degree of
   randomness is a little less than that of most `'natural''
   knot-arrangements.}},
ISSN = {{0021-4795}},
Unique-ID = {{ISI:A1993LM28800006}},
}

