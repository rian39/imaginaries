@article{ ISI:000081337000010,
Author = {Guist-Desprairies, F},
Title = {{The collective imaginary or the construction of the world in instituted
   groups}},
Journal = {{REVUE FRANCAISE DE PSYCHANALYSE}},
Year = {{1999}},
Volume = {{63}},
Number = {{3}},
Pages = {{863+}},
Month = {{JUL-SEP}},
Abstract = {{In this paper, the author interrogates the different modalities of the
   passage between psychic reality and the external world, between the
   individual imaginary and social imaginary significations, within the
   process of the institutionalisation of groups.
   The preeminence attributed to the imaginary as the foundation both of
   socio-history and of the psyche have led him to posit, at the origin of
   the micro-social link, the construction of a dynamic system of
   representations or collective imaginary which is a system of
   interpretation, a principle of ordering combining the affective needs
   of individuals with the functional logic of organisations.
   Processes of construction and deconstruction are considered to reside
   within an inter-subjectivity envisaged in its link with practice, that
   is to say, via the need to transform the reality that in part founds
   being together, cooperation.
   These ideas lead to the question of intervention in the instituted
   groups that are undergoing a crisis and have asked for aid.}},
ISSN = {{0035-2942}},
Unique-ID = {{ISI:000081337000010}},
}

