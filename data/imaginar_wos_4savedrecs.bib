@article{ ISI:000078653400012,
Author = {Gufler, H},
Title = {{Witchcraft beliefs among the Yamba (Cameroon)}},
Journal = {{ANTHROPOS}},
Year = {{1999}},
Volume = {{94}},
Number = {{1-3}},
Pages = {{181-198}},
Abstract = {{Witchcraft beliefs are part and parcel of the traditional religion of
   the Yamba. They provide an explanation of social conflict and
   misfortune but also of chance, prosperity, and good fortune. Three
   major categories of witchcraft and its mechanisms are described:
   cannibal witchcraft, leopard transform, and witch-breeze. This is
   followed by a description of the different anti-witchcraft devices,
   which the Yamba employ to defend themselves against the maleficient
   activities of witches. But not all categories of witchcraft are feared
   and disapproved of. The journey of some witches to the witch-market and
   to an imaginary land to the east explain the experience of chance and
   good fortune. It becomes clear that Yamba witchcraft beliefs are a
   combination of ``the fruit of human imagination and early notions of
   personality modified by various social structures{''} (E. M. Chilver).}},
ISSN = {{0257-9774}},
Unique-ID = {{ISI:000078653400012}},
}

@article{ ISI:000078855400003,
Author = {Stavem, K},
Title = {{Willingness to pay: a feasible method for assessing treatment benefits
   in epilepsy?}},
