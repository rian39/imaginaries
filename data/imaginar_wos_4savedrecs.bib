@article{ ISI:A1996VL96900001,
Author = {Loewenstein, B},
Title = {{Animal symbolicum?}},
Journal = {{FILOSOFICKY CASOPIS}},
Year = {{1996}},
Volume = {{44}},
Number = {{4}},
Pages = {{535-564}},
Abstract = {{This article considers the ever-present nature of symbols in the human
   world and their specific nature in comparison with discursive signs,
   considering the `'use and harm (good and bad) of symbolic
   orientations'' in secular society. Symbols are dramatised images which
   contain deeply rooted inherited experience and recall forgotten
   practices for certain situations, reinforce the system of faith and
   other forms of collective identity. The article also looks al the
   symbolic quality of the subjects and activities of everyday life - from
   home and car to football. Those recreational activities, which fall
   within the symbolic rule of a limited field, seem to have an eminent
   civilising function. Symbols do however create an imaginary field of
   power, coherence, legitimacy, a certain order, a barrier against chaos,
   `'a sign of quality''. It is natural for revolutions to begin with the
   devaluing of accepted symbolic systems. Nationalism also represents a
   symbolic control of the field, since it creates a collective identity
   by using `'narrative symbols'', i.e. emotionally loaded experiences or,
   at times, mythical narratives, and of course inclusive codes which
   exclude acid stigmatise other groups. It is the staging of a sacred
   drama of redemption which is an expression of unity.
   With the help of mythical images, symbols can liberate areas tamed by
   civilisation, but at the same time they can represent a sign of the
   diversity of spaces and of human experience, and show how little
   self-evident are our routine orientations. It is clearly those traces
   of an archaic past in us all that allow people to break out with mass
   symbols; it is the `'forgotten language'' which in times of crisis
   breaks through the thin layer of the taught, the rational, the
   sublimated. At the same time even the most modern system looks to its
   past for confirmation, so that the struggles against seemingly
   regressive non-discursive signs can be counter-productive. Our frenetic
   horizontal movement must inevitably include something of the
   `'vertical'' nature of symbolic acts, which confirms the validity of
   one or the other type of social relationship. Without living examples
   the credibility of these relationships is of course itself only an
   uncertain symbol. Every sign is liable to falsification through life.}},
ISSN = {{0015-1831}},
Unique-ID = {{ISI:A1996VL96900001}},
}

@article{ ISI:A1996VL90500004,
Author = {Deltete, RJ and Guy, RA},
Title = {{Emerging from imaginary time}},
Journal = {{SYNTHESE}},
Year = {{1996}},
Volume = {{108}},
Number = {{2}},
Pages = {{185-203}},
Month = {{AUG}},
Abstract = {{Recent models in quantum cosmology make use of the concept of imaginary
   time. These models all conjecture a join between regions of imaginary
   time and regions of real time. We examine the model of James Hartle and
   Stephen Hawking to argue that the various `no-boundary' attempts to
   interpret the transition from imaginary to real time in a logically
   consistent and physically significant way all fail. We believe this
   conclusion also applies to `quantum tunneling' models, such as that
   proposed by Alexander Vilenkin. We conclude, therefore, that the notion
   of `emerging from imaginary time' is incoherent. A consequence of this
   conclusion seems to be that the whole class of cosmological models
   appealing to imaginary time is thereby refuted.}},
ISSN = {{0039-7857}},
Unique-ID = {{ISI:A1996VL90500004}},
}

