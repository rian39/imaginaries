@article{ ISI:A1992JL04700002,
Author = {KLIPPEL, B and KUDLEK, G and LAZARUSMAINKA, G},
Title = {{ANXIETY AND THE DESCRIPTION OF THE EVENT EXAMINATION - SELF-REFERENCE
