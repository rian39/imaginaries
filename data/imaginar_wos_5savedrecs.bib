Title = {{GOLDBERG,EMANUEL, ELECTRONIC DOCUMENT-RETRIEVAL, AND BUSH,VANNEVAR MEMEX}},
Journal = {{JOURNAL OF THE AMERICAN SOCIETY FOR INFORMATION SCIENCE}},
Year = {{1992}},
