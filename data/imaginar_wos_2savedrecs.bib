@article{ ISI:000225331800002,
Author = {Looby, C},
Title = {{Southworth and seriality: `The hidden hand in the New York Ledger'}},
Journal = {{NINETEENTH-CENTURY LITERATURE}},
Year = {{2004}},
Volume = {{59}},
Number = {{2}},
Pages = {{179-211}},
Month = {{SEP}},
Abstract = {{E.D.E.N. Southworth's originally massively popular novel The Hidden
   Hand (1859), was not published as a book until it had undergone
   serialization three separate times, over the course of a course of a
   quarter century, in a weekly story-paper called the New York Ledger.
   This bare fact about the material form in which it circulated and
   gained its large and admiring audience has consequences for
   interpreting the novel that have gone entirely unexamined by scholars.
   Thus, Southworth's novel offers a good test for the claim that the
   material form of publication (in this case, periodical serialization)
   is a substantively important aspect of the work's meaning: the readers
   who responded to it so enthusiastically had all read it under the
   conditions of seriality. In this essay I attempt to take account of the
   interpretive implications of seriality for understanding Southworth's
   novel, exploring some of the ways in which it was ideologically
   embedded in the Ledger and describing the active and canny role it
   played in that periodical apoliticism. The Hidden Hand embodies the
   magazine's declared nonpartisanship in, among other things, various
   forms of generic melding and imaginary scenarios of sectional
   unification. Critical accounts that have sought to characterize this
   novel as proslavery or antislavery have all missed the points that
   Southworth's tale actively ignores such political issues and thus
   sought to transcend the dire sectional controversies that were
   unmistastakably impending at the time of its publication.}},
ISSN = {{0891-9356}},
Unique-ID = {{ISI:000225331800002}},
}

@article{ ISI:000225281700007,
Author = {Disselkamp, M},
Title = {{An alien Germany and an innate Rome, a critique of received ideas,
   cultural criticism and classical visions of Rome by J.J. Winckelmann}},
Journal = {{NEOPHILOLOGUS}},
Year = {{2004}},
Volume = {{88}},
Number = {{4}},
Pages = {{569-585}},
Month = {{OCT}},
Abstract = {{This article takes a selection of the letters of Johann Joachim
   Winckelmann as its starting point in order to analyse the dialectic of
   the tension in his work between the cultural reality of Germany, which
   is perceived to be inferior, and the ideal alterity of Rome.
   Winckelmann portrays the insignificance of the cultural setting within
   which his academic socialisation took place - in other words, the lack
   of aesthetic offerings in the city and university of Halle - with such
   scathing accuracy that this connoisseur of fine art who was trained in
   Rome seems to become a stranger and exile in his own time and place. In
   contrast to this, the imaginary, classical art-world of Rome appears to
   be the natural home of aesthetic scholars. The Roman ideal, however,
   does not replace Halle and the German academy - it remains instead the
   measure by which both are judged. This interaction between one's own
   present reality and alterity opens up various possibilities for a gain
   in self-knowledge. Moreover, the destruction of the present contains a
   subtle salvation for the same.}},
ISSN = {{0028-2677}},
Unique-ID = {{ISI:000225281700007}},
}

@article{ ISI:000228595700004,
