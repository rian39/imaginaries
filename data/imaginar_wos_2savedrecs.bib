@article{ ISI:000237097900001,
Author = {Ulliac, G},
Title = {{Narrowing of imaginary world concerning dream states and dream}},
Journal = {{EVOLUTION PSYCHIATRIQUE}},
Year = {{2006}},
Volume = {{71}},
Number = {{1}},
Pages = {{1-9}},
Month = {{JAN-MAR}},
Abstract = {{The history of the Psychiatry contains the idea of narrow links between
   dreams and madness. Then, the question is to know what is imaginary,
   and what happens to the man who dreams or enters a dream state. One can
   notice that naturalist descriptions - those of the psychiatric clinics
   - despite their ideal of objectivity and exactness, do not help us to
   answer the question. We are therefore obliged to put aside this
   descriptive attitude and attempt a phenomenological explanation. This
   leads us to show the essential poverty, of the imaginary world.
   Nevertheless, one could not literally take up Sartre's words ``There is
   no imaginary world{''} literally. In support of Heidegger's lessons old
   1929-1930, one would rather say that, while sinking into imaginary
   world, man is poor in his real world. A being-hi-the-world, the Dasein
   is what his world is; the one growing poorer, and the other vanishing
   with him. Thus, dreams and madness constitue an existential narrowing.
   As far as they constitute, man has acted upon the real existential
   disaster. It would be an illusion to believe in the preservation of
   complete deep personality, in any sort of interiority. There is no
   inner man. There is only one being in the world. This is an opportunity
   to evaluate all the poverty of Psychiatry and other medical
   disciplines. (c) 2005 Elsevier SAS. Tous droits reserves.}},
DOI = {{10.1016/j.evopsy.2005.06.010}},
ISSN = {{0014-3855}},
Keywords = {{dream states; phenomenology; imaginary; being-in-the-world; Dasein}},
Unique-ID = {{ISI:000237097900001}},
}

