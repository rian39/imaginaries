
@article{ ISI:000252180300005,
Author = {Lesec, Cedric},
Title = {{``Zodiac is a great thing, now...{''}}},
Journal = {{REVUE DE L ART}},
Year = {{2007}},
Number = {{157}},
Pages = {{39-46}},
Month = {{SEP}},
Abstract = {{In the 1950s, Catholic thought was challenged by a ``quarrel of sacred
   art.{''} Radical dogmatic positions confronted each other around the
   question of the ``regeneration{''} of religious art and the position of
   the church faced with the artistic evolutions of the 20(th) century,
   and especially the appearance of abstract art. A group of Benedictine
   monks seized upon the occasion of this uncertainty to propose an
   enlightened approach, defined in an art journal, and then in a
   veritable editorial adventure, the launching of Zodiaque, whose
   original function was to provide, as a source of inspiration to
   contemporary artists the models offered by romanesque art. This would
   provide the source that their speculative search could trust. They
   would find a base in this art, which the director of publications, Dom
   Angelico Surchamp, considered more authentic.
   From an artistic and dogmatic engagement, the Editions Zodiaque
   gradually became a highly successful editor of fine arts publications.
   Their approach, both systematic and synthetic of the sites they
   proposed, offered an extremely original synoptic vision for a period
   which up to then had only been known to the average person, but also to
   art historians, through the major masterpieces, or through the most
   easily accessible sites.
   The analysis of the history of these publications provides insight into
   the role played by photography in the diffusion and in the transmission
   of knowledge in art history, in the constitution of Andre Malraux's
   ``imaginary museum.{''} Malraux was a staunch supporter of these
   publications, answer to his appeal.}},
ISSN = {{0035-1326}},
Unique-ID = {{ISI:000252180300005}},
}

