@article{ ISI:000234431400021,
Author = {Mercier, M},
Title = {{A theatricalized nord: The rhetoric of the region in the ceremonial
   discourse accompanying the Kings' and Emperors' entrances from the
   XVIIth to the XIXth century}},
Journal = {{REVUE DU NORD}},
Year = {{2005}},
Volume = {{87}},
Number = {{360-61}},
Pages = {{577+}},
Month = {{APR-SEP}},
Abstract = {{The entrances of the King or the Emperor in a town or a province,
   staged as memory's theatre, turn out to be complex semiotic objects, as
   much through the motivations of their sponsors, as through the spatial
   or intellectual origins of their designers and through the various
   degrees of perception of those taking part, be they actors or
   spectators. This complex superimposition of discourses and designs,
   harmonious or discordant, generates the image of a transfigured
   territory, of an imaginary North. From Louis XIV to Napoleon III, from
   Valenciennes to Dunkirk and from Lille to Arras, triumphal arches,
   parades and speeches made by city or region officials yield above all
   the picture of a North attached to the ceremonials of entrances as
   means to demonstrate its fidelity to a centralized power. Then, through
   the organization of the costly festive events and the large scale
   mobilization of all the local skills, one can gradually make out the
   image of a welcoming and attractive North. This appeal through the
   display of farm products and the litany of industrial shows confirms
   the theme of prosperity that will assert itself in the region's
   rhetoric ceremony after ceremony.}},
ISSN = {{0035-2624}},
Unique-ID = {{ISI:000234431400021}},
}

@article{ ISI:000234585200006,
Author = {Klein, O and Demoulin, S and Licata, L and Lambert, S},
Title = {{``If you know he is an engineer, I don't need to tell you he is
   smart{''}: The influence of stereotypes on the communication of social
   knowledge}},
Journal = {{CAHIERS DE PSYCHOLOGIE COGNITIVE-CURRENT PSYCHOLOGY OF COGNITION}},
Year = {{2004}},
Volume = {{22}},
Number = {{4-5}},
Pages = {{463-478}},
Month = {{OCT}},
Abstract = {{This study examines the influence of an audience's knowledge of the
   group membership of a target on the communication of information about
   this target. Participants were asked to rank order five traits to
   describe a person labelled as an engineer to an imaginary audience.
   Overall they were more likely to choose traits stereotypical of the
   target's social category when the audience was presented as unaware
   than aware of this category membership. Within-participants analyses
   also indicated that the perceived stereotypicality of a trait predicted
   its communication more positively when the audience was unaware than
   aware of the target's social category. These results can be interpreted
   in line with the maxim of quantity (Grice, 1975): stereotype-consistent
   information being more informative when the audience is unaware than
   aware of the target's social category, it is more likely to be
   communicated in the former than in the latter case.}},
