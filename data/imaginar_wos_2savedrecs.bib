Title = {{A zoomorphic Chinese bronze from the late Warring States period
   (4th-3rd century BC) donated to the Musee Guimet}},
Journal = {{REVUE DES MUSEES DE FRANCE-REVUE DU LOUVRE}},
Year = {{2005}},
Number = {{3}},
Pages = {{31+}},
Month = {{JUN}},
Abstract = {{Zoomorphic bronze recipients (zun or xizun) for fermented liquor to be
   ritually offered to ancestors or spirits, or served to guests at solemn
   feasts, have been found from the Shang period (c. 13(th) century BC) up
   until the Warring States period (c.3(rd) century BC),
   Unconventional in form and relatively rare- in comparison with other
   pieces of ritual bronze ware-, they have always proved difficult to
   categorize, and, as a result, have been regarded as little more than
   curiosities. Nevertheless, over tell centuries, and despite their
   peculiar shape, such recipients provide a faithful mirror of evolving
   stylistic trends.
   Two major groups can be immediately distinguished-one in which a
   particular species of animal is represented with astonishing accuracy;
   and a second where a variety of zoological features are combined to
   represent purely imaginary creatures. Analysis of the genealogy of the
   zoomorphic zun recently donated to the Musee Guimet collections has
   shown clearly that they belong to the final phase of the latter group.}},
ISSN = {{0035-2608}},
Unique-ID = {{ISI:000230274100010}},
}

@article{ ISI:000231953500002,
Author = {Roberts, J},
Title = {{The power of the `imaginary' in disciplinary processes}},
Journal = {{ORGANIZATION}},
Year = {{2005}},
Volume = {{12}},
Number = {{5}},
Pages = {{619-642}},
Month = {{SEP}},
Abstract = {{The focus of this article is on processes of social control and the
   role that images and processes of identification play in effecting such
   control. The article begins by schematically tracing the history of the
   new paradigm of control firstly within the managerial literature as a
   move `from control to commitment', then through critical accounts
   of,employee subjection', through to more recent discussions of the
   nature of resistance. This history and its various puzzling inversions
   are then reviewed through Lacan's account of the `imaginary', and
   Butler's associated re-reading of oedipal identifications. These offer
   an account both of the constitution of a humanist, interiorized sense
   of self-of the self as a discrete, autonomous, independent entity-as
   well as of the illusions or misunderstandings of this humanist
   conception of the self. It is suggested that the Power of the imaginary
   lies in the power of recognition and the way in which this acts as a
   lure or trap in which we seek and find ourselves. It is this desire for
   existence confirming recognition that founds our interest in the
   control of others and leaves us vulnerable to control by others.
   Understanding the misrecognition involved in such processes of
   identification allows clearer sight of the conditions for effective
   resistance.}},
DOI = {{10.1177/1350508405055938}},
ISSN = {{1350-5084}},
Keywords = {{control; identification; identity; Lacan; resistance; self-discipline}},
Keywords-Plus = {{CORPORATE GOVERNANCE; LABOR PROCESS; ORGANIZATIONS; RESISTANCE;
   IDENTITY; ACCOUNTABILITY; IDENTIFICATION; SUBJECTIVITY; MANAGEMENT;
   DISCOURSE}},
Unique-ID = {{ISI:000231953500002}},
}

