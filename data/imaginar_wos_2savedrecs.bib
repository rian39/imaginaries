@article{ ISI:000232426800002,
Author = {Madrid, AL},
Title = {{Imagining modernity, revising tradition: Nor-tec music in tijuana and
   other borders}},
Journal = {{POPULAR MUSIC AND SOCIETY}},
Year = {{2005}},
Volume = {{28}},
Number = {{5}},
Pages = {{595-618}},
Month = {{DEC}},
Abstract = {{Based on extensive fieldwork in Tijuana, San Diego, Los Angeles, and
   Mexico City, this article explores the intersections of identity,
   modernity, desire, and marginality in the production, distribution, and
   transnational consumption of Nor-tec music. Tijuana musicians developed
   Nor-tec by combining sounds sampled from traditional music of the north
