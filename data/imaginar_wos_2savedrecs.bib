Year = {{2005}},
Volume = {{19}},
Number = {{3}},
Pages = {{331+}},
Month = {{SEP}},
Abstract = {{This article develops an alternative argument for understanding
   hysteria as mimesis within a phenomenological reading of
   psychoanalysis. Deconstructing arguments on sexual difference, gender
   performance and the masquerade, this article draws on works by Judith
   Butler, Luce Irigaray and Maurice Merleau-Ponty to argue for a more
   embodied imaginary that can encompass a psychic world composed of fluid
   mimetic selves. In contrast to this is the hysteric who masquerades
   within a fixed mimesis of Oedipal sexual difference. This mimesis is a
   disassociated rivalry or melodramatic performance that masquerades as
   femininity and masculinity and defends against a more fluid world of
   bodily affects. Implicit in this account are two claims: first, that
   the unconscious is understood as phenomenological affect; second, that
   Oedipal repression and hysterical disassociation are synonymous.}},
DOI = {{10.1080/09502360500196318}},
ISSN = {{0950-236X}},
Keywords = {{mimesis; hysteria; phenomenological; imaginary; Oedipal;
   embodied/disembodied}},
Unique-ID = {{ISI:000232084800006}},
}

@article{ ISI:000231802600004,
