@article{ ISI:000249064600003,
Author = {Louth, Charlie},
Title = {{Morike's lucid intervals (Eduard Morike)}},
Journal = {{OXFORD GERMAN STUDIES}},
Year = {{2007}},
Volume = {{36}},
Number = {{1}},
Pages = {{21-33}},
Abstract = {{Both in his introductory poem An einem Wintermorgen' and in the
   Spillner fragment, Morike presents moments which seem to hesitate
   between night and day. Such vacillations between past and future not
   only serve to indicate the continuities between German classicism and
   romanticism in his work, but they also indicate how his poetry grows
   out Of such moments, whether positive or negative, which connote an
   emptying of the self Though these things can be related to Morike's
   biography, or to the temper of the age in which he lived, they
   ultimately show how poetry itseif animates a moment which is both
   inside and outside of time, resisting the pressure of reality. Morike's
   poetry is especially a work of memory and commemoration; this can be
   seen in poems about artefacts (Auf eine Lampe) or natural phenomena
   (Auf eine Christblume), where a transitional imaginary moment allows
   the blurring of the possible and the impossible. A differently
   presented moment) in Abreise', show space transforming into a time, and
   produces what can only be a rare and fleeting coincidence of poetry and
   life.}},
DOI = {{10.1179/174592107X224178}},
ISSN = {{0078-7191}},
Unique-ID = {{ISI:000249064600003}},
}

@article{ ISI:000249064600006,
Author = {Duttlinger, Carolin},
Title = {{Morike's fossils: The poetics of palaeontology (Eduard Morike)}},
Journal = {{OXFORD GERMAN STUDIES}},
Year = {{2007}},
Volume = {{36}},
Number = {{1}},
Pages = {{60-75}},
Abstract = {{Eduard Morike's keen interest in palaeontology is reflected in two
   poems which both address changing conceptions of natural time and their
   impact on religion, memory and imagination in the nineteenth century.
   Within the guise of a light-hearted Gelegenheitsgedicht, Morike's `Der
   Petrefaktensammler' explores the gulf between geological theories and
   human experience; the speaker's enjoyment of nature is curtailed by
   fossils acting as reminders of the relative brevity of human life. This
   realisation is replayed within a more allegorical context in Morike's
   poem `Gottliche Reminiscenz'. Here a fossil features as the centrepiece
   of an imaginary painting of the young Christ which brings together the
   realms of religion and science not as conflicting paradigms but as two
   models of explanation which can still be harmonised. Despite their
   apparent differences, then, the article argues that `Der
   Petrefaktensammler' and `Gottliche Reminiscenz' provide complementary
   insights into Morike's mobilisation of palaeontology as a tool of his
   poetic imagination.}},
DOI = {{10.1179/174592107X224204}},
ISSN = {{0078-7191}},
Unique-ID = {{ISI:000249064600006}},
}

